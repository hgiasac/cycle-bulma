{
  "defaultSeverity": "warning",
  "extends": ["tslint:latest"],
  "rules": {
    "quotemark": [true, "single"],
    "jsx-no-lambda": true,
    "curly": true,
    "max-line-length": {
      "severity": "error",
      "options": 140
    },
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  },
  "jsRules": {
    "curly": true
  },
  "rulesDirectory": []
}
